{
  "name": "jcore-cli",
  "version": "3.3.0",
  "main": "src/main.ts",
  "scripts": {
    "test": "biome check ./src",
    "build": "esbuild src/main.ts --bundle --minify --platform=node --outfile=bin/jcore",
    "watch": "esbuild src/main.ts --bundle --watch  --minify --sourcemap --platform=node --outfile=bin/jcore",
    "format": "biome check --apply ./src",
    "preversion": "pnpm test",
    "version": "",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@sentry/node": "^7.38.0",
    "adm-zip": "^0.5.10",
    "chalk": "^5.3.0",
    "follow-redirects": "1.15.3",
    "inquirer": "^9.2.12",
    "semver": "^7.3.8",
    "smol-toml": "^1.1.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.1",
    "@types/adm-zip": "^0.5.0",
    "@types/follow-redirects": "^1.14.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^18.11.18",
    "@types/semver": "^7.5.6",
    "esbuild": "^0.16.17",
    "typescript": "^4.9.5"
  }
}
