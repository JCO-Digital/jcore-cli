{
  "name": "jcore-cli",
  "version": "0.99.10",
  "scripts": {
    "build": "esbuild main.ts --bundle --platform=node --outfile=bin/jcore",
    "watch": "esbuild main.ts --bundle --watch  --minify --sourcemap --platform=node --outfile=bin/jcore",
    "patch": "npm version patch --no-git-tag-version --force && git commit -a -m "
  },
  "config": {
    "branch": "fluffycritter"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/node": "^18.11.18",
    "esbuild": "^0.16.17",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "adm-zip": "^0.5.10"
  }
}
